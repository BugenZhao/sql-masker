  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch SQL Example",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd",
      "cwd": "${workspaceFolder}",
      "args": [
        "-d",
        "example/min/ddl",
        "-p",
        "example/min/prepare",
        "sql",
        "-f",
        "example/min/mask.sql"
      ]
    },
    {
      "name": "Launch Event Example",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd",
      "cwd": "${workspaceFolder}",
      "args": [
        "-d",
        "example/tpcc/ddl",
        "event",
        "-i",
        "example/tpcc/events",
        "-o",
        "out/tpcc"
      ]
    }
  ]
}
