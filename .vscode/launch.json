  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch SQL Example",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd",
      "cwd": "${workspaceFolder}",
      "args": [
        "--ddl",
        "example/min/execute.sql",
        "sql",
        "-f",
        "example/min/mask.sql"
      ]
    },
    {
      "name": "Launch Event Example",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd",
      "cwd": "${workspaceFolder}",
      "args": [
        "--ddl",
        "example/tpcc/execute.sql",
        "event",
        "-f",
        "example/tpcc/events.tsv"
      ]
    }
  ]
}
